  <view class='flex-row flex-center margin-right-30'>
    <view class='flex-3'></view>
    <image src='{{uploadpath}}video/{{xiangqinlist.touxiang}}' class='icon-50 radius-50'></image>
  </view>


  <view style='height:5vh'></view>

